<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#78909C</item>
        <item name="colorPrimaryDark">#546E7A</item>
        <item name="colorAccent">#FF4085</item>

    </style>

    <style name="MyTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#37474F</item>
        <item name="colorPrimaryDark">#263238</item>
        <item name="colorAccent">#FF4085</item>

    </style>


    <style name="AppTheme.NoActionBar">
        <item name="android:theme">@android:style/Theme.Light.NoTitleBar</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="RadioButton" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:button">@color/colorPrimary</item>
    </style>
</resources>
